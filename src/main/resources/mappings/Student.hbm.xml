<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
						           "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-cascade="save-update">
	<class name="unq.arq.model.Student">
		<id column="ID" name="id">
			<generator class="native"/>
		</id>
		<property name="username" not-null="true"></property>
		<property name="password" not-null="true"></property>
		<property name="name" not-null="true"></property>
		<property name="lastName" not-null="true"></property>
		<property name="dni" not-null="true"></property>
	</class>
</hibernate-mapping>